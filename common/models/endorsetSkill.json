{
  "name": "endorsetSkill",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "skillScore": {
      "type": "number",
      "required": true
    },
    "avgSkillScore": {
      "type": "number"
    },
    "created": {
      "type": "date"
    },
    "modified": {
      "type": "date"
    },
    "userDetailId": {
      "type": "number",
      "required": true,
      "index": true
    },
    "skillId": {
      "type": "number",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "userDetail": {
      "type": "belongsTo",
      "model": "userDetail",
      "foreignKey": "userDetailId"
    },
    "skill": {
      "type": "belongsTo",
      "model": "skill",
      "foreignKey": "skillId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
