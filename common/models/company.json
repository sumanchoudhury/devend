{
  "name": "company",
  "plural": "companies",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "companyName": {
      "type": "string",
      "required": true
    },
    "companyNumber": {
      "type": "string",
      "required": true
    },
    "industry": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "country": {
      "type": "string",
      "required": true
    },
    "userDetailId": {
      "type": "number",
      "required": true,
      "index": true
    },
    "created": {
      "type": "date"
    },
    "modified": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "userDetail": {
      "type": "belongsTo",
      "model": "userDetail",
      "foreignKey": "userDetailId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
